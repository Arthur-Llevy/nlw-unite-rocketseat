// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}


// Criar model da tabela
model Event {

  id String @id @default(uuid())
  title String 
  details String? // campo opicional
  slug String @unique
  maximumAtteends Int? @map("maximum_atteends")

  // @@map("events") - renome√°-la
}